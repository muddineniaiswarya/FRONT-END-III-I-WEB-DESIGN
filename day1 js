        console.log(typeof price);
        console.log(typeof inf);
        console.log(typeof notNumber);
        console.log(age);
        console.log(price);
        console.log(inf);
        console.log(notNumber);
        //string
        let name='john';
        let greeting="hello!";
        let template=`hi ${name},welcome!`;
        console.log(typeof name);
        console.log(name);
        //boolean
        let isLoggedin=true;
        let isAdult=false;
        console.log(typeof isLoggedin);
        //undefined
        let x;
        console.log(x);
        console.log(typeof x);
        //null
        let y=null;
        console.log(y);
        console.log(typeof y);
        //symbol
        let id=Symbol('id');
        let id2=Symbol('id');
        console.log(id===id2);
        console.log(typeof id);
        //big int
        let bigNumber=12345n;
        console.log(typeof bigNumber);

