let arr=[250,645,300,900,50];
let d;
// let j;
// let stu=d[j];
for(i=0;i<arr.length;i++){
    // d=arr[i]%10;
     d=arr[i]*(10/100);
     arr[i]=arr[i]-d;
    console.log(d);
    // stu=d;
    console.log(`${arr[i]}`);
}
