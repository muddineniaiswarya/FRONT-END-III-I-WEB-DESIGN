//slice
let fruit=["mango","banana","grapes","apple"];
let newfruit=fruit.slice(1,3);
console.log(fruit);
console.log(newfruit);//doesnot change real array
//splice
let fruit1=["mango","banana","grapes","apple"];
fruit1.splice(0,2,102,103);
console.log(fruit1);//change real array
fruit1.splice(3,0,109);
console.log(fruit1);
