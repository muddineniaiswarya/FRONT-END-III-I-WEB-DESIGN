package aiswarya2;
import java.util.Scanner;
public class Small {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("enter a number:");
		int i=sc.nextInt();
		int digit=9;
		
		while(i!=0) {
			 int d=i%10;
			if(d<digit) {
			digit=i;				
			}
			i=i/10;

		}
		System.out.println(digit);
		sc.close();
		
	}

}
