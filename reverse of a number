package aiswarya2;
import java.util.Scanner;

public class Rev {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("enter a number");
		int n=sc.nextInt();
		int rev=0;
		while(n!=0) {
	     rev=n%10;
	     n=n/10;
	     System.out.println(rev);
		}
		
        sc.close();
	}

}
